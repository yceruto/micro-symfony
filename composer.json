{
    "name": "yceruto/micro-symfony",
    "description": "Micro-Symfony Tools",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MicroSymfony\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MicroSymfony\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Yonel Ceruto",
            "email": "yonelceruto@gmail.com"
        }
    ],
    "require": {
        "php" : ">=8.1",
        "symfony/http-kernel": "^6.0",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.0",
        "symfony/yaml": "^6.0",
        "symfony/expression-language": "^6.0"
    }
}
